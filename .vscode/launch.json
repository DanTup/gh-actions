{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Run Setup Dart",
			"program": "${workspaceFolder}/src/setup-dart/main.ts",
			"env": {
				"RUNNER_TEMP": "/tmp/github-actions",
				"INPUT_CHANNEL": "stable"
			},
			"preLaunchTask": "npm: watch",
			"outFiles": [
				"${workspaceRoot}/out/**/*.js"
			]
		}
	]
}
